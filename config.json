{
    "name":"StrimUpOpen",
    "user":"root",
    "pass":"@123456",
    "dialect":"mysql",
    "host":"localhost",
    "tables":{
        "name":"person",
            
            "attributes":{
                "firstName": {
                "type": "Sequelize.STRING",
                "allowNull": false
                },
                "lastName": {
                    "type": "Sequelize.STRING",
                    "allowNull": false
                },
                "email": {
                    "type": "Sequelize.STRING",
                    "validate": {
                    "isEmail": true
                    }
                }
            }
       
    }
}